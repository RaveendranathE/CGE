<h2>Recent estimates</h2>
<p>There are
  <%= estimates.query.length %> estimates. <br/>
    <a href="/estimate/create" class="btn btn-primary">Add a new one</a></p>
<% for (var i = 0; i < estimates.query.length; i++) { estimate = estimates.query[i]; %>
  <div class="panel panel-default">
    <div class="panel-heading"></div>
    <div class="text-muted pull-right"></div>
    <div class="panel-body">
      ID:
      <%=  estimate._id %><br>
        <% include ../estimateparts/est-about %>
          <% include ../estimateparts/est-footage %>
            <% include ../estimateparts/est-flooring %>
              <% include ../estimateparts/est-roofing %>
                <% include ../estimateparts/est-waterproofing %>
                  <% include ../estimateparts/est-aggregate %>
                    <% include ../estimateparts/est-labor %>
                      <% include ../estimateparts/est-mileage %>
                        <% include ../estimateparts/est-misc %>
                          <% include ../estimateparts/est-comment %>
                            <% include ../estimateparts/est-result %> <br/>
                              <a href="/estimate/delete/<%= estimate._id  %>" class="btn btn-danger"> Delete </a>
                              <a href="/estimate/details/<%= estimate._id %>" class="btn btn-info"> Details </a>
                              <a href="/estimate/edit/<%= estimate._id %>" class="btn btn-warning">  Edit </a>
    </div>
  </div>
  <% } %>