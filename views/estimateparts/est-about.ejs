 <script type="text/javascript"> 
 function init_map() {
var tempMap = document.getElementsByClassName("gmap_canvas");
console.log("tempmap was: " + tempMap);
 for(var x =0;x<tempMap.length;x++){
      var myOptions = {
      scrollwheel: false,
       zoom: 17,
        center: new google.maps.LatLng(<%=estimate.latitude%>, <%=estimate.longitude%>),
        mapTypeId: google.maps.MapTypeId.SATELLITE };
     map = new google.maps.Map(tempMap[x], myOptions);
     marker = new google.maps.Marker({ map: map, position: new google.maps.LatLng(<%=estimate.latitude%>, <%=estimate.longitude%>) 

});
     infowindow = new google.maps.InfoWindow({ content: "<b><%=estimate.address%>"  }); 
     google.maps.event.addListener(marker, "click", function () { 
    //  infowindow.open(map, marker); 
     }); 
     infowindow.open(map, marker); 
 } 
     } google.maps.event.addDomListener(window, 'load', init_map);
     
</script>
 </div>